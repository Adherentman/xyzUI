{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "import",
        "jsx-a11y",
        "prettier"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "import/core-modules": ["react"]
    },
    "rules": {
        "import/no-unresolved": 2,
        "import/no-extraneous-dependencies": "off",
        "react/jsx-filename-extension": "off",
        "react/prefer-stateless-function": "off",
        "semi": "off"
    }
}