{
  "presets": [["@babel/preset-env", {
		"targets": {
			"browsers": [
				"last 2 versions",
				"Firefox ESR",
				"> 1%",
				"ie >= 9",
				"iOS >= 8",
				"Android >= 4"
			]
		}
	}],
	"@babel/preset-react"],
	"plugins": [
		"emotion",
		"react-hot-loader/babel",
		"@babel/plugin-proposal-object-rest-spread"
	]
}
